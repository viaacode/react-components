{
  "name": "@meemoo/react-components",
  "version": "2.16.4",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "repository": "github:viaacode/react-components",
  "homepage": "https://github.com/viaacode/react-components#readme",
  "author": "Andry Charlier <andry.charlier@studiohyperdrive.be>",
  "contributors": [
    "Andry Charlier <andry.charlier@studiohyperdrive.be>",
    "Ian Emsens <ian.emsens@studiohyperdrive.be>",
    "Bavo Vanderghote <bavo.vanderghote@studiohyperdrive.be>",
    "Bert Verhelst <bert.verhelst@studiohyperdrive.be>"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "preversion": "npm run lint && npm test && npm run type-check",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "build-and-copy-to-client": "rimraf ../hetarchief-client/node_modules/@meemoo/react-components/dist && npm run build && cp -r dist package.json ../hetarchief-client/node_modules/@meemoo/react-components",
    "build:analyze": "npm run build -- --config-analyze",
    "build-storybook": "build-storybook -o build",
    "build-storybook:with-sourcemaps": "cross-env GENERATE_SOURCEMAP=true build-storybook -o build",
    "format": "npm run lint:ts -- --fix",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:scss": "stylelint 'src/**/*.{css,scss}' --allow-empty-input",
    "start": "start-storybook -p 3020 --ci",
    "test": "jest --cache",
    "test:watch": "npm run test -- --watch",
    "test:output": "cross-env CI=true jest --json --outputFile=.storybook/test-results.json",
    "type-check": "tsc --noEmit",
    "licenses": "license-checker --csv --out licenses.csv",
    "snyk-protect": "snyk protect"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "^16.14.0 || ^17.0.2",
