    "Bert Verhelst <bert.verhelst@studiohyperdrive.be>"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "vite build",
    "copy-to-avo-client": "rimraf ../avo2-client/node_modules/@meemoo/react-components/dist ../avo2-client/node_modules/.cache  && cp -r dist package.json ../avo2-client/node_modules/@meemoo/react-components",
    "copy-to-hetarchief-client": "rimraf ../hetarchief-client/node_modules/@meemoo/react-components/dist ../hetarchief-client/node_modules/.cache ../hetarchief-client/.next && cp -r dist package.json ../hetarchief-client/node_modules/@meemoo/react-components",
    "copy-to-admin-core": "rimraf ../react-admin-core-module/ui/node_modules/@meemoo/react-components/dist ../react-admin-core-module/ui/node_modules/.cache && cp -r dist package.json ../react-admin-core-module/ui/node_modules/@meemoo/react-components",
    "copy": "npm run build && npm run copy-to-avo-client && npm run copy-to-hetarchief-client && npm run copy-to-admin-core",
    "build:analyze": "npm run build -- --config-analyze",
    "build-storybook": "build-storybook -o build",
    "build-storybook:with-sourcemaps": "cross-env GENERATE_SOURCEMAP=true build-storybook -o build",
    "format": "npm run lint:ts -- --fix",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:scss": "stylelint 'src/**/*.{css,scss}' --allow-empty-input",
    "start": "start-storybook -p 3020 --ci",
    "test": "jest --cache",
    "test:watch": "npm run test -- --watch",
    "test:output": "cross-env CI=true jest --json --outputFile=.storybook/test-results.json",
    "type-check": "tsc --noEmit",
    "licenses": "license-checker --csv --out licenses.csv",
    "snyk-protect": "snyk protect",
    "bump": "bump-package-versions --part=patch --strategy=highest ./package.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "@flowplayer/player": "3.3.0",
