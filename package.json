{
  "name": "@meemoo/react-components",
  "version": "2.3.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "repository": "github:viaacode/react-components",
  "homepage": "https://github.com/viaacode/react-components#readme",
  "author": "Andry Charlier <andry.charlier@studiohyperdrive.be>",
  "contributors": [
    "Andry Charlier <andry.charlier@studiohyperdrive.be>",
    "Ian Emsens <ian.emsens@studiohyperdrive.be>",
    "Bavo Vanderghote <bavo.vanderghote@studiohyperdrive.be>"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "force-resolutions": "npx npm-force-resolutions",
    "analyze": "source-map-explorer 'build/*.js'",
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "build-storybook": "build-storybook -o build",
    "build-storybook:with-sourcemaps": "cross-env GENERATE_SOURCEMAP=true build-storybook -o build",
    "format": "npm run lint -- --fix",
    "preicons": "node scripts/icons.js src/icons",
    "icons": "rimraf src/components/Icon/Icons && svgr -d src/components/Icon/Icons src/icons --ext ts",
    "posticons": "named-exports --dir=src/components/Icon/Icons && renamer --find .ts --replace .tsx \"./src/components/Icon/Icons/**\"",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:scss": "stylelint 'src/**/*.{css,scss}' --allow-empty-input",
    "start": "start-storybook -p 3020 --ci",
    "test": "jest --cache",
    "test:v1": "jest --cache --config src/v1/jest.config.js",
    "test:all": "npm run test -- --projects jest.config.js src/v1/jest.config.js",
    "test:output": "cross-env CI=true jest --json --outputFile=.storybook/test-results.json",
    "licenses": "license-checker --csv --out licenses.csv",
    "snyk-protect": "snyk protect"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
